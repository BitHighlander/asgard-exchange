    "cipher-base": "^1.0.4",
    "crypto-browserify": "^3.12.0",
    "ethers": "^5.0.29",
    "express": "^4.17.1",
    "i": "^0.3.6",
    "json-loader": "^0.5.7",
    "npm": "^6.14.8",
    "os": "^0.1.1",
    "rxjs": "~6.5.4",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.0.0",
    "yargs": "^13.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",

{
  "name": "asgard-exchange",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/setenv.ts",
    "patchCosmos": "node ./scripts/fix.sr25519.js",
    "start": "npm run config -- --environment=dev --configuration=testnet && npm run patchCosmos && ng serve",
    "build": "npm run config -- --environment=prod --configuration=chaosnet && npm run patchCosmos && ng build --prod",
    "build:testnet": "npm run config -- --environment=prod --configuration=testnet && npm run patchCosmos && ng build --configuration testnet",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-prod": "node server.js",
    "docker:build": "docker build -t thorchain/asgard-exchange:latest .",
    "docker:run": "docker run --env-file=.env thorchain/asgard-exchange:latest",
    "docker:tag": "docker tag thorchain/asgard-exchange:latest registry.digitalocean.com/pioneer/thorchain/asgard-exchange:latest",
    "docker:push": "docker push registry.digitalocean.com/pioneer/thorchain/asgard-exchange:latest",
    "docker:push:all": "npm run docker:build && npm run docker:tag && npm run docker:push"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@binance-chain/javascript-sdk": "^4.1.1",
    "@pioneer-platform/pioneer-sdk": "0.0.5",
    "@thorchain/asgardex-binance": "^3.0.1",
    "@thorchain/asgardex-token": "^0.1.1",
    "@thorchain/asgardex-util": "^0.8.1",
    "@trustwallet/walletconnect": "0.0.12",
    "@walletconnect/client": "^1.2.2",
    "@walletconnect/qrcode-modal": "^1.2.2",
    "@xchainjs/xchain-binance": "latest",
    "@xchainjs/xchain-bitcoin": "latest",
    "@xchainjs/xchain-client": "latest",
    "@xchainjs/xchain-cosmos": "^0.11.0",
    "@xchainjs/xchain-crypto": "latest",
    "@xchainjs/xchain-ethereum": "latest",
    "@xchainjs/xchain-thorchain": "latest",
    "@xchainjs/xchain-util": "latest",
    "angularx-qrcode": "^10.0.11",
